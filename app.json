{
  "expo": {
    "name": "Aether Mobile",
    "slug": "aether-mobile",
    "scheme": "aether",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0b0f14"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.aether.mobile",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "INTERNET"
      ],
      "allowBackup": false
    },
    "extra": {
      "eas": {
        "projectId": "ec670257-abba-4e64-a737-71e6e6581ab1"
      }
    }
  }
}
